{
    "AcademicLecture": {
        "category": "Education",
        "icon": "SchoolIcon",
        "label": "Academic Lectures",
        "description": "Transcribed academic lectures and educational content",
        "color": "primary",
        "sections": {
            "header": {
                "title": {
                    "text": "Academic Lectures",
                    "variant": "h6"
                },
                "subtitle": {
                    "text": "transcripts",
                    "variant": "body2"
                },
                "icon": {
                    "size": "2.8rem",
                    "background": {
                        "opacity": 0.12,
                        "hover": 0.16
                    }
                }
            },
            "topics": {
                "title": "Popular Topics",
                "type": "chips",
                "maxItems": 5,
                "chip": {
                    "size": "small",
                    "background": {
                        "opacity": 0.08,
                        "hover": 0.12
                    }
                }
            },
            "recentItems": {
                "title": "Recent Lectures",
                "type": "list",
                "maxItems": 3,
                "emptyState": {
                    "text": "No lectures available",
                    "icon": "SchoolIcon"
                },
                "itemLayout": {
                    "icon": "DescriptionIcon",
                    "iconSize": "1.2rem",
                    "primary": {
                        "field": "title",
                        "variant": "body2",
                        "fontWeight": 500,
                        "maxLines": 1
                    },
                    "secondary": {
                        "fields": ["date", "duration"],
                        "separator": " • ",
                        "variant": "caption",
                        "color": "text.secondary",
                        "opacity": 0.7
                    },
                    "status": {
                        "field": "status",
                        "type": "chip",
                        "variants": {
                            "Complete": {
                                "color": "success",
                                "variant": "outlined",
                                "icon": "CheckCircleIcon"
                            },
                            "Processing": {
                                "color": "warning",
                                "variant": "outlined",
                                "icon": "AccessTimeIcon"
                            },
                            "Failed": {
                                "color": "error",
                                "variant": "outlined",
                                "icon": "ErrorIcon"
                            }
                        }
                    }
                }
            }
        },
        "styles": {
            "header": {
                "backgroundColor": "primary.light",
                "textColor": "primary.main",
                "iconColor": "primary.main",
                "avatarBackgroundOpacity": 0.12,
                "backgroundOpacity": 0.04,
                "hoverBackgroundOpacity": 0.08,
                "borderRadius": "8px 8px 0 0"
            },
            "content": {
                "padding": 2.5,
                "gap": 2.5,
                "backgroundColor": "background.paper",
                "chipBackgroundColor": "primary.light",
                "chipTextColor": "primary.main",
                "itemHoverColor": "primary.light",
                "iconColor": "primary.main",
                "borderRadius": "0 0 8px 8px"
            }
        }
    },
